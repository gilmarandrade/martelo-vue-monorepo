{
    "name": "Martelo Devtools Extension",
    "description": "Martelo Devtools Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": ["<all_urls>"]
        }
    ],
    "background": {
        "service_worker": "workers/background.js"
    }
  }